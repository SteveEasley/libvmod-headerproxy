varnishtest "Test vmod not configured"

server s1 {
    rxreq
    txresp
} -start

varnish v1 -vcl+backend {
    import headerproxy from "${vmod_topbuild}/src/.libs/libvmod_headerproxy.so";

    sub vcl_recv {
        headerproxy.call();
    }

    sub vcl_backend_fetch {
        headerproxy.process();
    }

    sub vcl_backend_response {
        headerproxy.process();
    }

    sub vcl_deliver {
        headerproxy.process();
    }

    sub vcl_synth {
        headerproxy.process();
    }

    sub vcl_pipe {
        headerproxy.process();
    }
} -start

client c1 {
    txreq -url "/"
    rxresp
} -run

varnish v1 -expect n_object == 1
varnish v1 -expect cache_miss == 1
